<div class="jumbotron">
  <h1 class="display-3" align="center">Send Us Your Address</h1>
  <p class="lead" align="center" >and we'll let you know where you are</p>
  <div id="form_address_wrapper">
    <%= render 'places/new/form_search_address' %>
  </div>

  <div class="container">
    <div class="row">
      <div class="col">
        <div id="map" class="col" style='width: 100%; height: 400px; border: solid 1px;'>
        </div>
      </div>
    </div>
  </div>


  <%= content_for :javascripts do %>
    <script src="//maps.google.com/maps/api/js?key=<%= ENV['GOOGLE_MAP_API_KEY']%>"></script>
  <% end %>
